{
  "name": "gjsx-broadway",
  "private": true,
  "type": "module",
  "scripts": {
    "compile": "node esbuild.mjs",
    "deploy": "docker stack deploy -c system/gjsx-stack.yml gijsx",
    "rm": "docker stack remove gijsx",
    "ide": "docker stack deploy -c system/development-stack.yml dev_workspace",
    "image": "docker-compose -f docker-compose.yml",
    "traefik": "docker stack deploy -c system/traefik/swarm-config.yml traefik",
    "deploy-fresh": "yarn image build; yarn image push; yarn deploy",
    "watch": "node esbuild.mjs --watch",
    "log": "docker service log",
    "log-rm": "sudo rm -rf log/*"
  },
  "keywords": [
    "typescript",
    "gtk4",
    "gnome",
    "broadway",
    "jsx",
    "docker"
  ],
  "version": "1.2.2",
  "repository": "git@github.com:bresnow/gjsx-broadway-docker.git",
  "author": "bresnow <bresnow@fltngmmth.com>",
  "license": "MIT",
