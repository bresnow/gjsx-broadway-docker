    "chainlocker": "^1.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dockerode": "^3.3.4",
    "dockerode-compose": "^1.3.2",
    "express": "^4.18.2",
    "gun": "^0.2020.1238",
    "http-proxy": "^1.18.1",
