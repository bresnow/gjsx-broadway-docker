    "@types/react-dom": "^18.0.8",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.11",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "@remix-run/express": "^1.8.2",
