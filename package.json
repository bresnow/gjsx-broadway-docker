{
    "name": "relay",
    "private": true,
    "scripts": {
        "stack": "docker stack deploy -c docker-stack.yml relay",
        "image": "docker-compose -f docker-compose.yml",
        "start": "gjs _build/main.js",
        "compile": "tsc -p tsconfig.json",
        "rollup": "rollup --inlineDynamicImports --no-treeshake ./_compiled/entry.js  --file ./_build/main.js --format es"
    },
    "keywords": [
        "typescript",
        "gtk",
        "gnome",
        "broadway"
    ],
    "version": "1.0.0",
    "repository": "git@github.com:bresnow/relay_servers.git",
    "author": "bresnow <bresnow@me.com>",
    "license": "MIT",
    "devDependencies": {
        "@gi-types/cairo1": "^1.0.1",
        "@gi-types/gtk": "^3.24.9",
        "@gi-types/webkit2": "^2.30.8",
        "rollup": "^2.38.0"
    }
}
