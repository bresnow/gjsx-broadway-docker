{
  "name": "proxy",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"yarn build:remix\" \"yarn build:server\" \"yarn build:css\"",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server/index.ts --outdir=./server ",
    "build:css": "yarn generate -m",
    "watch": "concurrently \"yarn watch:remix\" \"yarn watch:server\" \"yarn watch:css\"",
    "watch:remix": "remix watch",
    "watch:server": "yarn build:server -- --watch",
    "watch:css": "yarn generate -- -w",
    "generate": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "yarn test -- --update",
    "lint": "eslint app --ext ts,tsx",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@remix-run/express": "1.7.0",
    "@remix-run/node": "1.7.0",
    "@remix-run/react": "1.7.0",
    "chainlocker": "^1.3.2",
    "compression": "1.7.4",
    "dockerode": "^3.3.4",
    "dockerode-compose": "^1.3.2",
    "express": "^4.18.2",
    "gun": "^0.2020.1238",
    "http-proxy": "^1.18.1",
    "isbot": "^3.6.5",
    "js-yaml": "^4.1.0",
    "lz-string": "^1.4.4",
    "morgan": "1.10.0",
    "object-assign": "^4.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redaxios": "^0.5.1",
    "tar-fs": "^2.1.1",
    "transparent-proxy": "^1.9.1",
    "zx": "^7.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.7.0",
    "@remix-run/eslint-config": "1.7.0",
    "@remix-run/serve": "1.7.0",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/http-proxy": "^1.17.9",
    "@types/js-yaml": "^4.0.5",
    "@types/jsesc": "^3.0.1",
    "@types/lz-string": "^1.3.34",
    "@types/morgan": "1.9.3",
    "@types/object-assign": "^4.0.30",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-test-renderer": "18.0.0",
    "autoprefixer": "10.4.8",
    "concurrently": "7.3.0",
    "esbuild": "0.15.7",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-tailwindcss": "3.6.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.2",
    "vitest": "0.23.0"
  },
  "engines": {
    "node": ">=16"
  }
}
