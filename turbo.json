{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "compile": {
            "outputs": [
                "gi_modules/_compiled/*"
            ]},
        "build": {
            "dependsOn": ["^compile"],
            "outputs": [
                "_compiled/*",
                "broadway-proxy/build/*",
                "broadway-proxy/public/build/**/*"
            ]
        },
        "lint": {
            "outputs": []
        }
    }
}